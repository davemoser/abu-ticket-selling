{
  "name": "abu-ticket-selling",
  "main":"lib/main.js",
  "bin":"lib/main.js",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "start": "node ./lib/main.js",
    "bundle": "npm run build && pkg package.json"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "nodemailer": "^6.9.15",
    "pdf-to-printer": "^5.6.0",
    "typescript": "^5.6.3",
    "unix-print": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/nodemailer": "^6.4.16",
    "@yao-pkg/pkg": "^6.0.0"
  },
  "pkg": {
    "scripts": "lib/**/*.js",
    "assets":["node_modules/pdf-to-printer/**/*"],
    "targets": [ "node20-win-x64" ],
    "outputPath": "dist"
  }
}
